{
  "name": "backend",
  "version": "1.5.0",
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npm run clean && npm run compile",
    "compile": "npx tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/app.js\"",
    "start": "node dist/app.js",
    "test": "NODE_ENV=test jest",
    "test:cov": "NODE_ENV=test jest --coverage",
    "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear -d ./src/config/database.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/migration -d ./src/config/database.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.3",
    "@types/express-fileupload": "^1.5.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^7.2.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "inversify": "^7.10.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "yamljs": "^0.3.0"
  }
}
