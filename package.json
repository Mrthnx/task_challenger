{
  "name": "backend",
  "version": "1.2.1",
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npm run clean && npm run compile",
    "compile": "npx tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/app.js\"",
    "start": "node dist/app.js",
    "test": "jest --watch",
    "test:cov": "jest --coverage",
    "typeorm:cache": "npx typeorm-ts-node-commonjs cache:clear -d ./src/config/database.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/migration -d ./src/config/database.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "inversify": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21"
  }
}
